{
  "name": "time-study-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "start": "npm run lint && npm run build && npm run serve",
    "start:dev": "npm run lint && npm run build:dev && npm run serve",
    "test": "vue-cli-service test:unit",
    "migrate": "node ./node_modules/knex/bin/cli.js migrate:latest",
    "seed": "node ./node_modules/knex/bin/cli.js seed:run",
    "local-migrate": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest dotenv_config_path=./local.env",
    "local-seed": "node -r dotenv/config ./node_modules/knex/bin/cli.js seed:run dotenv_config_path=./local.env"
  },
  "dependencies": {
    "axios": "0.19.0",
    "bcrypt": "3.0.6",
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.0-rc.27",
    "core-js": "2.6.5",
    "eslint-friendly-formatter": "4.0.1",
    "lodash.orderby": "4.6.0",
    "match-sorter": "4.0.1",
    "moment": "2.24.0",
    "socket.io": "2.2.0",
    "style-loader": "0.23.1",
    "uuid": "3.3.2",
    "v-datatable-light": "0.8.0",
    "vue": "2.6.10",
    "vue-bootstrap4-table": "1.1.10",
    "vue-class-component": "7.0.2",
    "vue-cli-plugin-vuetify": "0.6.1",
    "vue-loader": "15.7.1",
    "vue-native-websocket": "2.0.13",
    "vue-progress": "0.2.1",
    "vue-property-decorator": "8.1.0",
    "vue-radial-progress": "0.2.10",
    "vue-router": "3.0.3",
    "vue-simple-progress": "1.1.0",
    "vue-simple-spinner": "1.2.8",
    "vue-spinkit": "1.4.1",
    "vue-spinner": "1.0.3",
    "vue-star-rating": "1.6.1",
    "vue-teible": "^0.9.21",
    "vuetify": "2.0.3",
    "vuetify-loader": "1.3.0",
    "vuex": "3.1.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/chai": "4.1.0",
    "@types/mocha": "5.2.4",
    "@types/node": "12.0.10",
    "@vue/cli-plugin-babel": "3.8.0",
    "@vue/cli-plugin-typescript": "3.8.0",
    "@vue/cli-plugin-unit-mocha": "3.8.0",
    "@vue/cli-service": "3.8.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "chai": "4.1.2",
    "gulp": "3.9.1",
    "gulp-sourcemaps": "2.6.4",
    "gulp-typescript": "4.0.1",
    "node-sass": "4.13.1",
    "sass-loader": "7.1.0",
    "typescript": "3.4.3",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
